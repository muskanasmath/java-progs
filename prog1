#Write a program to convert "a=b;c=d;e=f;g=h" to [("a","b"),("c","d"),("e","f"),("g","h")]

def input_str():
    str1 = input("enter str: ")
    return str1

    
def setconvert():   #tuples
    str2 = input_str()
 
    
    a = str2.split(";")
    result=[]
    # a is a list
    for r in a:
        b = r.split("=")
        result.append(b)
    return result
    
print(setconvert())

